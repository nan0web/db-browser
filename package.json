{
	"name": "@nan0web/db-browser",
	"version": "1.0.0",
	"description": "Browser Database client as extension of @nan0web/db",
	"main": "src/index.js",
	"types": "types/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "tsc",
		"test": "node --test \"src/**/*.test.js\"",
		"test:coverage": "node --test --experimental-test-coverage src/",
		"clean": "rm -rf .cache/ && rm -rf dist/",
		"clean:modules": "rm -rf node_modules",
		"test:docs": "node --test src/README.md.js",
		"test:release": "node --test \"releases/**/*.test.js\"",
		"test:coverage:collect": "nan0test coverage",
		"test:status": "nan0test status",
		"precommit": "npm test",
		"prepush": "npm test",
		"prepare": "husky",
		"playground": "node playground/main.js"
	},
	"keywords": [
		"nan0",
		"database",
		"browser",
		"authentication",
		"http",
		"json"
	],
	"author": "ЯRаСлав YaRaSLove <support@yaro.page>",
	"license": "ISC",
	"packageManager": "pnpm@10.11.0",
	"devDependencies": {
		"@nan0web/auth-core": "workspace:*",
		"@nan0web/db": "workspace:*",
		"@nan0web/http": "workspace:*",
		"@nan0web/types": "workspace:*",
		"@nan0web/test": "workspace:*",
		"husky": "^9.1.7"
	},
	"peerDependencies": {
		"@nan0web/auth-core": "^1.0.0",
		"@nan0web/db": "^1.0.0",
		"@nan0web/http": "^1.0.0",
		"@nan0web/types": "^1.0.0"
	}
}
